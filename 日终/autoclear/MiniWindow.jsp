<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<style type="text/css">
.colorBlock{left:0;top:0;width:10px;height:10px;border-style:solid;border-width:1px;}
</style>
<title>颜色状态框</title>
</head>
<body>
<table>
  <tr>
    <td>
      <div id="color1" style="background-color:#D9D6C3;" class="colorBlock">
      </div>
    </td>
    <td>
      <font size=2 color="#0C212B"><b>未激活</b></font>
    </td>
  </tr>
  <tr>
    <td>
      <div id="color1" style="background-color:#2A5CAA;" class="colorBlock">
      </div>
    </td>
    <td>
      <font size=2 color="#0C212B"><b>已激活</b></font>
    </td>
  </tr>
  <tr>
    <td>
      <div id="color1" style="background-color:#FF00FF;" class="colorBlock">
      </div>
    </td>
    <td>
      <font size=2 color="#0C212B"><b>作业暂停</b></font>
    </td>
  </tr>
  <tr>
    <td>
      <div id="color1" style="background-color:#00FF00;" class="colorBlock">
      </div>
    </td>
    <td>
      <font size=2 color="#0C212B"><b>作业完成</b></font>
    </td>
  </tr>
  <tr>
    <td>
      <div id="color1" style="background-color:#FF0000;" class="colorBlock">
      </div>
    </td>
    <td>
      <font size=2 color="#0C212B"><b>作业失败</b></font>
    </td>
  </tr>
  <tr>
    <td>
      <div id="color1" style="background-color:#8552A1;" class="colorBlock">
      </div>
    </td>
    <td>
      <font size=2 color="#0C212B"><b>作业中断</b></font>
    </td>
  </tr>
  <tr>
    <td>
      <div id="color1" style="background-color:#FFFF00;" class="colorBlock">
      </div>
    </td>
    <td>
      <font size=2 color="#0C212B"><b>正在处理</b></font>
    </td>
  </tr>
</table>
</body>
</html>